<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="AppQrZahlteil" lang="java">

    <one-to-one ref="com.axelor.apps.base.db.App" name="app"/>

    <string name="apiKey" max="32"/>
    <string name="encryptionKey" max="32"/>
    <string name="initializationVector" max="16"/>

    <track>
      <field name="apiKey"/>
      <field name="encryptionKey"/>
      <field name="initializationVector"/>
    </track>
  </entity>

</domain-models>
