<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <form name="app-xippo-qrzahlteil-config-form" title="App QR Zahlteil"
    model="com.axelor.apps.base.db.AppQrZahlteil" canDelete="false" canNew="false" width="large">
    <panel name="mainPanel">
      <field name="apiKey"/>
      <field name="encryptionKey"/>
      <field name="initializationVector"/>
    </panel>

    <panel-mail name="mailPanel">
      <mail-messages limit="4"/>
      <mail-followers/>
    </panel-mail>
  </form>

</object-views>
